||(n%100>0&&n%100<20))?1:2);},'ga':function(n){return(n==1)?0:((n==2)?1:((n<7)?2:((n<11)?3:4)));},'gd':function(n){return(n==1||n==11)?0:(n==2||n==12)?1:(n>2&&n<20)?2:3;},'sl':function(n){return(n%100==1)?0:((n%100==2)?1:((n%100==3||n%100==4)?2:3));},'pl':function(n){return(n==1)?0:((n%10>=2&&n%10<=4&&(n%100<10||n%100>=20))?1:2);},'csb':function(n){return(n==1)?0:((n%10>=2&&n%10<=4&&(n%100<10||n%100>=20))?1:2);},'lt':function(n){return(n%10==1&&n%100!=11)?0:((n%10>=2&&(n%100<10||n%100>=20))?1:2);},'cy':function(n){return(n==0)?0:((n==1)?1:((n==2)?2:((n==3)?3:((n==6)?4:5))));},'hr':function(n){return(n%10==1&&n%100!=11)?0:((n%10>=2&&n%10<=4&&(n%100<10||n%100>=20))?1:2);},'mt':function(n){return(n==1)?0:((n==0||(n%100>1&&n%100<11))?1:((n%100>10&&n%100<20)?2:3));},'ar':function(n){return(n==0)?0:((n==1)?1:((n==2)?2:((n%100>=3&&n%100<=10)?3:((n%100>=11&&n%100<=99)?4:5))));}},getPluralRuleFunction:function(lang){if(typeof Plurals.pluralRules[lang]==="undefined"){var ruleFunction=Plurals.
